let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "matrix"
    , license = Registry.License.`Apache-2.0`
    , repository = Some
        (Registry.Repo.GitHub { owner = "jutaro", repo = "purescript-matrix" })
    , devDependencies =
        toMap
          { console = "^1.0.0"
          , assert = "^1.0.0"
          , benchotron = "^4.0.0"
          , random = "^1.0.0"
          }
    , dependencies =
        toMap
          { proxy = "^1.0.0"
          , arrays = "^1.0.0"
          , extensions = "^1.0.0"
          , vector = "^1.0.0"
          }
    }
