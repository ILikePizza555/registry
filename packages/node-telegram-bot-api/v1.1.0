let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "node-telegram-bot-api"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "justinwoo", repo = "purescript-node-telegram-bot-api" }
        )
    , devDependencies =
        toMap
          { node-process = "^5.0.0"
          , test-unit =
              "git@github.com:natefaubion/purescript-test-unit.git#fe237eb51c7e7a372252861a39f77dc6a4625582"
          , validation = "^3.1.0"
          }
    , dependencies =
        toMap
          { aff = "^4.0.0"
          , simple-json = "^1.0.0"
          , strings = "^3.0.0"
          , foreign = "^4.0.0"
          }
    }
