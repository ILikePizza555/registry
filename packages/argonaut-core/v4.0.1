let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "argonaut-core"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "purescript-contrib", repo = "purescript-argonaut-core" }
        )
    , devDependencies = toMap { quickcheck = "^5.0.0" }
    , dependencies =
        toMap
          { control = "^4.0.0"
          , either = "^4.0.0"
          , arrays = "^5.0.0"
          , maybe = "^4.0.0"
          , foreign-object = "^1.0.0"
          , tailrec = "^4.0.0"
          , gen = "^2.0.0"
          , prelude = "^4.0.0"
          , nonempty = "^5.0.0"
          , strings = "^4.0.0"
          , functions = "^4.0.0"
          }
    }
