let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "argonaut-core"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "purescript-contrib", repo = "purescript-argonaut-core" }
        )
    , devDependencies = toMap { strongcheck = "^1.0.0" }
    , dependencies =
        toMap { maps = "^1.0.0", enums = "^1.0.0", functions = "^1.0.0" }
    }
