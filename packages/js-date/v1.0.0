let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "js-date"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "purescript-contrib", repo = "purescript-js-date" }
        )
    , devDependencies =
        toMap { console = "^1.0.0", assert = "^1.0.0", globals = "^1.0.0" }
    , dependencies =
        toMap
          { exceptions = "^1.0.0", integers = "^1.0.0", datetime = "^1.0.0" }
    }
