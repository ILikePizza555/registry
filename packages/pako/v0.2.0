let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "pako"
    , license = Registry.License.MIT
    , repository = Some
        (Registry.Repo.GitHub { owner = "jacereda", repo = "purescript-pako" })
    , devDependencies =
        toMap
          { exceptions = "^3.1.0"
          , console = "^3.0.0"
          , debug = "^3.0.0"
          , quickcheck = "^4.6.0"
          }
    , dependencies =
        toMap
          { prelude = "^3.1.0"
          , simple-json = "^1.0.0"
          , strings = "^3.3.1"
          , arraybuffer = "^5.0.0"
          }
    }
