let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "eulalie"
    , license = Registry.License.`LGPL-3.0+`
    , repository = Some
        (Registry.Repo.GitHub { owner = "bodil", repo = "purescript-eulalie" })
    , devDependencies = toMap { test-unit = "~10.0.1", quickcheck = "~3.1.1" }
    , dependencies =
        toMap
          { control = "~2.0.0"
          , maybe = "~2.0.1"
          , monoid = "~2.2.0"
          , foldable-traversable = "~2.0.0"
          , integers = "~2.1.0"
          , globals = "~2.0.0"
          , tuples = "~3.1.0"
          , strings = "~2.1.0"
          , sets = "~2.0.0"
          }
    }
