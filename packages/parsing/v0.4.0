let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "parsing"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "purescript-contrib", repo = "purescript-parsing" }
        )
    , devDependencies = toMap { console = "^0.1.0", assert = "^0.1.0" }
    , dependencies =
        toMap
          { either = "^0.2.0"
          , arrays = "^0.4.0"
          , maybe = "^0.3.0"
          , foldable-traversable = "^0.4.0"
          , transformers = "^0.6.0"
          , strings = "^0.5.0"
          , identity = "^0.4.0"
          , lists = "^0.7.0"
          }
    }
