          , const = "^3.0.0"
          , validation = "^3.0.0"
          , partial = "^1.2.0"
          , strings = "^3.0.0"
          , sets = "^3.0.0"
          , lists = "^4.0.0"
          , datetime = "^3.0.0"
          }
    }
