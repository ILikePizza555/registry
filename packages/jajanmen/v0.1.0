    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "justinwoo", repo = "purescript-jajanmen" }
        )
    , devDependencies =
        toMap { console = "^4.1.0", assert = "^4.0.0", simple-json = "^4.0.0" }
    , dependencies =
        toMap
          { typelevel-prelude = "^3.0.0"
          , prelude = "^4.0.0"
          , node-sqlite3 = "^3.1.0"
          }
    }
