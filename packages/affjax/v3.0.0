          , unsafe-coerce = "^2.0.0"
          , refs = "^2.0.0"
          , integers = "^2.0.0"
          , math = "^2.0.0"
          , foreign = "^3.0.0"
          , http-methods = "^2.0.0"
          , nullable = "^2.0.0"
          }
