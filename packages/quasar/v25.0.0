let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "quasar"
    , license = Registry.License.`Apache-2.0`
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "slamdata", repo = "purescript-quasar" }
        )
    , devDependencies =
        toMap
          { node-fs-aff = "^4.0.0"
          , node-process = "^4.0.0"
          , node-child-process = "^4.0.0"
          , assert = "^3.0.0"
          }
    , dependencies =
        toMap
          { free = "^4.0.0"
          , pathy = "^4.0.0"
          , argonaut = "^3.0.0"
          , uri = "^3.0.0"
          , affjax-algebra = "^4.0.0"
          , oidc-crypt-utils = "^7.0.0"
          , nonempty = "^4.0.0"
          , ejson = "^9.0.0"
          }
    }
