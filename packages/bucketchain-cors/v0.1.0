let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "bucketchain-cors"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "Bucketchain", repo = "purescript-bucketchain-cors" }
        )
    , devDependencies = toMap { assert = "^4.0.0" }
    , dependencies =
        toMap
          { bucketchain = "^0.3.0"
          , bucketchain-header-utils = "^0.1.0"
          , http-methods = "^4.0.2"
          }
    }
