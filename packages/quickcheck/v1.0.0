    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "purescript", repo = "purescript-quickcheck" }
        )
    , dependencies =
        toMap
          { exceptions = "^1.0.0"
          , console = "^1.0.0"
          , either = "^1.0.0"
          , arrays = "^1.0.0"
          , transformers = "^1.0.0"
          , random = "^1.0.0"
          , strings = "^1.0.0"
          , lists = "^1.0.0"
          }
    }
