        toMap
          { profunctor = "^3.2.0"
          , record-extra = "^0.1.0"
          , debug = "^3.0.0"
          , test-unit = "^13.0.0"
          , psci-support = "^3.0.0"
          }
    , dependencies =
        toMap
          { variant = "^4.1.0"
          , console = "^3.0.0"
          , maybe = "^3.0.0"
          , parsing = "^4.3.1"
          , prelude = "^3.1.1"
          , record = "^0.2.6"
          , strings = "^3.5.0"
          }
    }
