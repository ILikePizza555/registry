let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "boomboom"
    , license = Registry.License.BSD-3-Clause
    , repository = Some
        (Registry.Repo.Git "https://github.com/paluh/purescript-boomboom.git")
    , devDependencies =
        toMap
          { debug = "^3.0.0", test-unit = "^13.0.0", psci-support = "^3.0.0" }
    , dependencies =
        toMap
          { variant = "^4.1.0"
          , console = "^3.0.0"
          , maybe = "^3.0.0"
          , parsing = "^4.3.1"
          , prelude = "^3.1.1"
          , strings = "^3.5.0"
          }
    }
