let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "echarts"
    , license = Registry.License.`Apache-2.0`
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "slamdata", repo = "purescript-echarts" }
        )
    , devDependencies =
        toMap
          { formatters = "^0.1.0"
          , now = "^2.0.0"
          , debug = "^2.0.0"
          , nonempty = "^3.0.0"
          , random = "^2.0.0"
          , signal = "^8.0.0"
          }
    , dependencies =
        toMap
          { control = "^2.0.0"
          , dom = "^3.1.0"
          , eff = "^2.0.0"
          , transformers = "^2.0.1"
          , foreign = "^3.0.0"
          , colors = "^2.0.0"
          }
    }
