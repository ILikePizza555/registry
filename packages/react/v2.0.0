let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "react"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "purescript-contrib", repo = "purescript-react" }
        )
    , devDependencies = toMap { console = "^2.0.0", psci-support = "^2.0.0" }
    , dependencies =
        toMap { prelude = "^2.1.0", unsafe-coerce = "^2.0.0", eff = "^2.0.0" }
    }
