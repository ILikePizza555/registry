let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "tailrec"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "purescript", repo = "purescript-tailrec" }
        )
    , devDependencies = toMap { console = "^3.0.0" }
    , dependencies =
        toMap
          { either = "^3.0.0"
          , st = "^3.0.0"
          , partial = "^1.2.0"
          , identity = "^3.0.0"
          }
    }
