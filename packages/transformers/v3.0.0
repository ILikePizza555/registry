    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "purescript", repo = "purescript-transformers" }
        )
    , devDependencies = toMap { console = "^3.0.0" }
    , dependencies =
        toMap
          { arrays = "^4.0.0"
          , distributive = "^3.0.0"
          , lazy = "^3.0.0"
          , tuples = "^4.0.0"
          }
    }
