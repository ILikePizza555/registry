let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "stringutils"
    , license = Registry.License.`Apache-2.0`
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "menelaos", repo = "purescript-stringutils" }
        )
    , devDependencies =
        toMap { console = "^3.0.0", strongcheck = "^3.0.0", lists = "^4.0.0" }
    , dependencies =
        toMap
          { either = "^3.0.0"
          , arrays = "^4.0.0"
          , maybe = "^3.0.0"
          , prelude = "^3.0.0"
          , integers = "^3.0.0"
          , partial = "^1.1.2"
          , strings = "^3.0.0"
          }
    }
