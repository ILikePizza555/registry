let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "stringutils"
    , license = Registry.License.`Apache-2.0`
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "menelaos", repo = "purescript-stringutils" }
        )
    , devDependencies =
        toMap { console = "^2.0.0", strongcheck = "^2.0.0", lists = "^3.0.0" }
    , dependencies =
        toMap
          { either = "^2.0.0"
          , arrays = "^3.0.0"
          , maybe = "^2.0.0"
          , prelude = "^2.1.0"
          , integers = "^2.0.0"
          , partial = "^1.1.2"
          , strings = "^2.0.1"
          }
    }
