    , dependencies =
        toMap
          { either = "^1.0.0"
          , arrays = "^1.0.0"
          , maybe = "^1.0.0"
          , prelude = "^1.0.1"
          , partial = "^1.1.2"
          , strings = "^1.0.0"
          }
    }
