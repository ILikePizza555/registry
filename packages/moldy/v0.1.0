let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "moldy"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "LiamGoodacre", repo = "purescript-moldy" }
        )
    , devDependencies = toMap { console = "^2.0.0", psci-support = "^2.0.0" }
    , dependencies =
        toMap
          { foldable-traversable = "^2.2.0"
          , prelude = "^2.3.0"
          , strings = "^2.1.0"
          }
    }
