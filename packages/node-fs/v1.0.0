let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "node-fs"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "purescript-node", repo = "purescript-node-fs" }
        )
    , devDependencies = toMap { console = "^1.0.0" }
    , dependencies =
        toMap
          { exceptions = "^1.0.0"
          , node-streams = "^1.0.0"
          , node-path = "^1.0.0"
          , unsafe-coerce = "^1.0.0"
          , node-buffer = "^1.0.0"
          , globals = "^1.0.0"
          , js-date = "^1.0.0"
          , foreign = "^1.0.0"
          , nullable = "^1.0.0"
          , datetime = "^1.0.0"
          }
    }
