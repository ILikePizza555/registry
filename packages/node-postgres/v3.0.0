let Registry = ../../RegistryV1.dhall

in  Registry.Package::{
    , name = "node-postgres"
    , license = Registry.License.MIT
    , repository = Some
        ( Registry.Repo.GitHub
            { owner = "epost", repo = "purescript-node-postgres" }
        )
    , devDependencies =
        toMap { generics = "^4.0.0", spec = "^1.0.0", js-date = "^4.0.0" }
    , dependencies =
        toMap
          { either = "^3.0.0"
          , foreign-generic = "^4.0.0"
          , arrays = "^4.0.0"
          , aff = "^3.0.0"
          , foldable-traversable = "^3.0.0"
          , prelude = "^3.0.0"
          , unsafe-coerce = "^3.0.0"
          , integers = "^3.0.0"
          , transformers = "^3.0.0"
          , foreign = "^4.0.0"
          , nullable = "^3.0.0"
          , datetime = "^3.0.0"
          }
    }
