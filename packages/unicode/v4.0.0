    , devDependencies =
        toMap
          { console = "^4.0.0"
          , spec = "^3.0.0"
          , random = "^4.0.0"
          , quickcheck = "^5.0.0"
          }
    , dependencies =
        toMap
          { maybe = "^4.0.0"
          , foldable-traversable = "^4.0.0"
          , strings = "^4.0.0"
          }
    }
